    "    def retrieve_shops_first_letter(self): \n",
    "        \"\"\"Get all letter links from the main page\"\"\"\n",
    "        base_url = 'https://www.mystore411.com'\n",
    "        html = urlopen(self.url)\n",
    "        bs = BeautifulSoup(html, 'html.parser')\n",
    "        urls = []\n",
    "        \n",
    "        for link in bs.find('div', {'id':'breadcrumb'}).find_all('a', href=re.compile('^(/store/)((?!:).)*$')):\n",
    "            if 'href' in link.attrs:\n",
    "                letters = link.attrs['href']\n",
    "                urls.append(letters)\n",
    "                new_urls = ['{}{}'.format(base_url,i) for i in urls]\n",
    "        return new_urls\n",
    "                    \n",
    "    def retrieve_stores(self):\n",
    "        \"\"\"Get all links of all shops\"\"\"\n",
    "        base_url = 'https://www.mystore411.com'\n",
    "        stores = []\n",
    "        for each in self.retrieve_shops_first_letter():\n",
    "            html = urlopen(each)\n",
    "            soup = BeautifulSoup(html, \"lxml\" )\n",
    "            for ul in soup.find_all('ol', class_='col2 d_store_list'):\n",
    "                for li in ul.find_all('li'):\n",
    "                    a = li.find('a')\n",
    "                    newnewurl = a['href']\n",
    "                    stores.append(newnewurl)\n",
    "                    all_stores_links = ['{}{}'.format(base_url,i) for i in stores]\n",
    "        return all_stores_links \n",
    "    \n",
    "    def retrieve_Canadian_shops(self):\n",
    "        \"\"\"Get all links which already includes the word \"Canada\"\"\"\"\n",
    "        Canada_list = [s for s in self.retrieve_Canadian_shops() if \"/Canada/\" in s]\n",
    "        return Canada_list\n",
    "    \n",
    "    def retrieve_others_shops(self):\n",
    "        \"\"\"Get all links which does not include the word \"Canada\"\"\"\"\n",
    "        stores_for_work = [s for s in self.retrieve_Canadian_shops() if \"/Canada/\" not in s]\n",
    "        others_stores = [re.sub(r'\\s+', '%20', x) for x in stores_for_work]\n",
    "        return others_stores\n",
    "    \n",
    "    def retrieve_links_from_side_menu(self):\n",
    "        \"\"\"Get all links of all shops of all counties from side menu\"\"\"\n",
    "        base_url = 'https://www.mystore411.com'\n",
    "        second_list = []\n",
    "        for each in self.retrieve_others_shops():\n",
    "            html = urlopen(each)\n",
    "            soup = BeautifulSoup(html, \"lxml\")\n",
    "            urlCanada = []\n",
    "            for ul in soup.find_all('ul', class_='sidemenu salmon'):\n",
    "                for li in ul.find_all('li'): \n",
    "                    a = li.find('a')\n",
    "                    urlSideMenu = a['href'] \n",
    "                    second_list.append(urlSideMenu)\n",
    "                    second_list_links = ['{}{}'.format(base_url,i) for i in second_Canada_list]\n",
    "        return second_list_links\n",
    "    \n",
    "    \"\"\"Divide all side menu links into 2 lists: already Canadian and not\"\"\"\n",
    "    \n",
    "    def retrieve_Canadian_shops_again(self):\n",
    "        \"\"\"Get all links which already includes the word \"Canada\"\"\"\"\n",
    "        second_Canada_list = [s for s in self.retrieve_links_from_side_menu() if \"/Canada/\" in s]\n",
    "        return second_Canada_list\n",
    "    \n",
    "    def join_Canadian_lists(self):\n",
    "        \"\"\"Join two lists with Canada inside link\"\"\"\n",
    "        third_Canada_list = self.retrieve_Canadian_shops() + self.retrieve_Canadian_shops_again()\n",
    "        return third_Canada_list\n",
    "    \n",
    "    def retrieve_provincies(self):\n",
    "        \"\"\"Get all links of all provinces inside all Canadian links\"\"\"\n",
    "        base_url = 'https://www.mystore411.com'\n",
    "        provinces = []\n",
